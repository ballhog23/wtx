mixin icon(name)
	case name
		when 'trophy'
			+iconTrophy()
		when 'crown'
			+iconCrown()
		when 'star'
			+iconStar()
		when 'users'
			+iconUsers()
		when 'user'
			+iconUser()
		when 'target'
			+iconTarget()
		when 'zap'
			+iconZap()
		when 'medal'
			+iconMedal()
		when 'trending-up'
			+iconTrendingUp()
		when 'skull'
			+iconSkull()
		when 'trending-down'
			+iconTrendingDown()
		when 'flame'
			+iconFlame()
		when 'swords'
			+iconSwords()
		when 'award'
			+iconAward()
		when 'chevron-down'
			+iconChevronDown()
		when 'basketball'
			+iconBasketball()
		when 'football'
			+iconFootball()
